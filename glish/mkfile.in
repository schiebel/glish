INCLUDE = -I.. -I$(SRC) -I$(SRC)/include -I$(INCDIR_P) $(PLUSINC)
CINCLUDE = -I.. -I$(SRC) -I$(SRC)/include -I$(INCDIR_P)

HDRS = $(H)/Dict.h $(H)/Object.h $(H)/Reporter.h $(H)/ValKern.h $(H)/glish.h \
	$(H)/Client.h $(H)/GlishType.h $(H)/Proxy.h $(H)/Str.h $(H)/Value.h \
	$(H)/Complex.h $(H)/List.h $(H)/Queue.h $(H)/Stream.h $(H)/VecRef.h

