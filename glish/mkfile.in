#IF SHARED
MK_LIB = @MAKE_SLIB@
LD_LIBS = @SLIB_LD_LIBS@
CFLAGS = @GLISH_FLAGS@ @CFLAGS@ @SLIB_CFLAGS@
CXXFLAGS = @GLISH_FLAGS@ @CXXFLAGS@ @SLIB_CXXFLAGS@
LIB_SUFFIX = @GLISH_SLIB_SUFFIX@
