GLIBS = $(LIBDIR_P)/libsosgc$(GSUFFIX) $(LIBDIR_P)/libeditgc$(GSUFFIX) $(LIBDIR_P)/libregxgc$(GSUFFIX) @NPD_LIB@
GCFLAG = -DENABLE_GC
MALLIB =
#ELSE
PLIB_P = $(LIBDIR_P)/libglishp$(SUFFIX)
