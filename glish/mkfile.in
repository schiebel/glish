CXXFLAGS = @GLISH_FLAGS@ @CXXFLAGS@
GSUFFIX = @GLISH_LIB_SUFFIX@
SUFFIX = @GLISH_LIB_SUFFIX@
RANLIB = @RANLIB@
LIBTGT = @STATIC_LIB@
BINTGT = @STATIC_BIN@
INSTALL_LIBTGT = INSTALL_@STATIC_LIB@
INSTALL_BINTGT = INSTALL_@STATIC_BIN@
GLISH_LIBS = @GLISH_LIBS@
INSTALL_LIBRARY = $(INSTALL_DATA)
#FI

#IF GC
